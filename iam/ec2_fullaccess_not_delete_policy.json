{
    "Version": "2012-10-17",
	"Statement": [
        {
            "Effect": "Allow",
            "Action": ["ec2:*", "ec2:Describe*", "ec2:Get*", "autoscaling:*", "elasticloadbalancing:*"],
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": ["ec2:DeleteVpc", "ec2:TerminateInstances"],
            "Resource": "*"
        }

    ]
}